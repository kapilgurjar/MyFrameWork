<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="My App test regression" thread-count="4" parallel="tests" verbose="4">


	<test name="Web Automation">
		<parameter name="browser" value="firefox"></parameter>

		<classes>
			<class name="com.qa.myframework.testscripts.GoogleTC" />
		</classes>
	</test>

	<test name="Web Automation on chrome">
		<parameter name="browser" value="chrome"></parameter>

		<classes>
			<class name="com.qa.myframework.testscripts.GoogleTC" />
		</classes>
	</test>

</suite>